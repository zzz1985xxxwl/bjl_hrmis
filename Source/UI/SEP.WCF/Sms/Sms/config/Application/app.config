<?xml version="1.0"?>
<configuration>
  <url>http://192.168.1.1/pages/confirmmesage.aspx</url>
  <PortNameConfig>COM4</PortNameConfig>
  <!--配置短信机波特率,通常无需修改-->
  <BautRateConfig>19200</BautRateConfig>
  <!--重发短信开始的时间点，默认为早上8点-->
  <EnableStartHour>8</EnableStartHour>
  <!--重发短信结束的时间点，默认为晚上22点-->
  <addEnableEndHour>22</addEnableEndHour>
</configuration>